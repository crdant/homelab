{
  "kind": "tm:ltm:virtual:virtualstate",
  "name": "${virtual_server}",
  "partition": "${partition}",
  "addressStatus": "yes",
  "autoLasthop": "default",
  "cmpEnabled": "yes",
  "connectionLimit": 0,
  "description": "PKS cluster ${cluster}",
  "destination": "/${partition}/${cluster_bigip_ip}:${port}",
  "enabled": true,
  "gtmScore": 0,
  "ipProtocol": "tcp",
  "mask": "255.255.255.255",
  "mirror": "disabled",
  "mobileAppTunnel": "disabled",
  "nat64": "disabled",
  "pool": "/${partition}/${pool}",
  "profiles": [
    {
      "kind": "tm:ltm:virtual:profiles:profilesstate",
      "name": "http",
      "partition": "Common",
      "context": "all"
    },
    {
      "kind": "tm:ltm:virtual:profiles:profilesstate",
      "name": "tcp-lan-optimized",
      "partition": "Common",
      "context": "all"
    },
    {
      "kind": "tm:ltm:virtual:profiles:profilesstate",
      "name": "${ssl_client_profile}",
      "partition": "${partition}",
      "context": "clientside"
    },
    {
      "kind": "tm:ltm:virtual:profiles:profilesstate",
      "name": "serverssl",
      "partition": "Common",
      "context": "serverside"
    }
  ],
  "rateLimit": "disabled",
  "rateLimitDstMask": 0,
  "rateLimitMode": "object",
  "rateLimitSrcMask": 0,
  "serviceDownImmediateAction": "none",
  "source": "0.0.0.0/0",
  "sourceAddressTranslation": {
    "pool": "/${partition}/${snat_pool}",
    "type": "snat"
  },
  "sourcePort": "preserve",
  "synCookieStatus": "not-activated",
  "translateAddress": "enabled",
  "translatePort": "enabled",
  "vlansEnabled": true,
  "vsIndex": 12,
  "vlans": [
    "/Common/external"
  ]
}
