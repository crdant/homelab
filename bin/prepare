#!/usr/bin/env bash
BASEDIR=`dirname $0`/..
. "${BASEDIR}/lib/env.sh"
. "${BASEDIR}/lib/generate_passphrase.sh"
set -e

if [ ! -d ${work_dir} ] ; then
  mkdir -p ${work_dir}
fi

if [ ! -d ${key_dir} ] ; then
  mkdir -p ${key_dir}
fi

aws s3api create-bucket --bucket ${state_bucket} --acl private --region ${aws_region}
